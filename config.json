{
  "agents": {
    "defaults": {
      "workspace": "~/.picoclaw/workspace",
      "model": "gemini-flash",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "model_list": [
    {
      "model_name": "gemini-flash",
      "model": "google/gemini-1.5-flash",
      "api_key": "PLACEHOLDER_GOOGLE_API_KEY"
    }
  ],
  "providers": {
    "google": {
      "api_key": "PLACEHOLDER_GOOGLE_API_KEY"
    }
  },
  "gateway": {
    "enable": true,
    "address": "0.0.0.0:7860"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "PLACEHOLDER_TELEGRAM_TOKEN",
      "allowFrom": ["PLACEHOLDER_TELEGRAM_USER"]
    }
  }
}
