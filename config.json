{
  "agents": {
    "defaults": {
      "workspace": "~/.picoclaw/workspace",
      "model": "llama-3.3-70b",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "model_list": [
    {
      "model_name": "llama-3.3-70b",
      "model": "groq/llama-3.3-70b-versatile",
      "api_key": "PLACEHOLDER_GROQ_API_KEY"
    }
  ],
  "providers": {
    "groq": {
      "api_key": "PLACEHOLDER_GROQ_API_KEY"
    }
  },
  "gateway": {
    "enable": true,
    "address": "0.0.0.0:7860"
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "PLACEHOLDER_TELEGRAM_TOKEN",
      "allowFrom": ["PLACEHOLDER_TELEGRAM_USER"]
    }
  }
}
